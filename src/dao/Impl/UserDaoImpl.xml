<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="dao.UserDao">
<select id="queryByUsername" parameterType="java.lang.String"  resultType="entity.User">
	select username,password from t_user where username = #{username}</select>
</mapper> 